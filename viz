// Membuat bounding box dari koordinat
var bounds = ee.Geometry.Rectangle([
  110.41278, // Bujur minimum (110° 24’ 46”)
  -7.5,      // Lintang minimum (7° 30’ 00” S → -7.5)
  110.81833, // Bujur maksimum (110° 49’ 06”)
  -7.06667   // Lintang maksimum (7° 4’ 00” S → -7.06667)
]);

// Tampilkan sebagai layer di peta
Map.centerObject(bounds, 10); // Zoom level 10 bisa disesuaikan
Map.addLayer(bounds, {color: 'red'}, 'Area Studi');
