  .expression('VV - VH', {
    VV: image_sen1.select('VV'),
    VH: image_sen1.select('VH'),
  })
  .rename('VV_VH_diff')
  .copyProperties(image_sen1, image_sen1.propertyNames());

var vv_vh_mean = image_sen1
  .expression('(VV + VH) / 2', {
    VV: image_sen1.select('VV'),
    VH: image_sen1.select('VH'),
  })
  .rename('VV_VH_mean')
  .copyProperties(image_sen1, image_sen1.propertyNames());

